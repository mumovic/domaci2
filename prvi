i=5;j=1;
x=[0.9*i, -0.9*i, -0.9*i, -0.9*i, -0.9*i, -1.1*i, 0.9*i, -0.7*i, 0.5*i, 0.2*i];
y=[-0.5*j, -1.0*j, 1.1*j, -0.2*j, 0.6*j, 0.0*j, -0.4*j, -1.5*j, -0.5*j, 0.6*j];
z=[x;y];
q=10;
xx=mean(x)
xy=mean(y)
xz=[xx;xy]
e11=0;
e22=0;
for i=1:q
e11=e11+x(i)*x(i);
e22=e22+x(i)*y(i);
end
e1=[e11/q;e22/q]
Ezz=(z*z')/10
Exx=mean(x.^2)
Exy=mean(x.*y)
